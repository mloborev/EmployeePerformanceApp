@model GetAllSelectionsViewModel

<a asp-action="Index">Home</a>


<h3>Top Users</h3>
<table class="table">
    <tr>
        <td>Surname</td>
        <td>Name</td>
        <td>Role</td>
        <td>Total</td>
    </tr>
    @foreach (var item in Model.DictTopUsers)
    {
        <tr>
            <td>@item.user.Surname</td>
            <td>@item.user.Name </td>
            <td>@item.user.Role.Name</td>
            <td>@item.mark</td>
        </tr>
    }
</table>


<h3>Bottom Users</h3>
<table class="table">
    <tr>
        <td>Surname</td>
        <td>Name</td>
        <td>Role</td>
        <td>Total</td>
    </tr>
    @foreach (var item in Model.DictBottomUsers)
    {
    <tr>
        <td>@item.user.Surname</td>
        <td>@item.user.Name </td>
        <td>@item.user.Role.Name</td>
        <td>@item.mark</td>
    </tr>
    }
</table>